{
	"@context": "https://schema.org",
	"@type": "TechArticle",
	"inLanguage": {
		"@type": "Language",
		"name": "English"
	},
	"headline": "<%= h1 %>",
	"description": "<%= description %>",
	"@id": "<%= url %>",
	"name": "<%= title %>",
	"datePublished": "<%= published_date %>",
	"dateModified": "<%= lastmod_date %>",
	"author": {
		"@type": "Person",
		"name": "<%= origin_author || author %>",
		"url": "https://techblog.example.com/about"
	},
	"mainEntityOfPage": {
		"@type": "WebPage",
		"@id": "<%= url %>#main-content"
	},
	"image": {
		"@type": "ImageObject",
		"url": "https://techblog.example.com/img/<% soc_img.name %>",
		"width": 1200,
		"height": 630
	},
	"keywords": "<%= keywords %>",
	"proficiencyLevel": "<%= proficiencyLevel %>",
	"dependencies": "<%= dependencies %>",
	"articleSection": "<%= articleSection %>",
	"about": {
		"@type": "Thing",
		"name": "<%= about %>"
	},
	"learningResourceType": "<%= learningResourceType %>",
	"audience": {
		"@type": "Audience",
		"audienceType": "Developers"
	},
	"genre": "Technical",
	<% if( name === 'index') { %>
	"breadcrumb": {
		"@type": "BreadcrumbList",
		"name": "Main navigation",
		"itemListElement": {
			"@type": "ListItem",
			"position": "1",
			"item": {
				"@id": "https://techblog.example.com/",
				"name": "Main page"
			}
		}
	}
	<% } else {%>
	"breadcrumb": {
		"@type": "BreadcrumbList",
		"name": "Main navigation",
		"itemListElement": [
			{
				"@type": "ListItem",
				"position": "1",
				"item": {
					"@id": "https://techblog.example.com/",
					"name": "Main page"
				}
			},
			{
				"@type": "ListItem",
				"position": "2",
				"item": {
					"@id": "<%= url %>",
					"name": "<%= title %>"
				}
			}
		]
	}
	<%} %>
}

